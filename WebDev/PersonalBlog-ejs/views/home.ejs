<%- include("partials/header") -%>

<h1>Home</h1>

<p><%- homeStartingContent %></p>

<% Posts.forEach(function(element) { %>

    <div>

        <h1>
            <%= element.title %>
        </h1>
        <p>
            <%= element.content.substring(0, 100)+ "...." %>
        </p>
        <a href= <%='/post/'+ element.title %> >
            Read More
        </a>

    </div>

    <% }) %>


<%- include("partials/footer") -%>